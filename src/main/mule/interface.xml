    <flow name="post:\bookings:application\json:car-partners-sapi-config">
		<flow-ref doc:name="addBookingFlow" doc:id="cd61702b-b0da-4e6b-b37d-92f9a375c5be" name="addBookingFlow"/>
    </flow>
</mule>
